{
   "name": "mk",
   "displayName": "mk",
   "description": "Extract PHONY targets of a all the Makefiles of the worspace (multi-projects), add 'mk' button to display a picker list filled with all targets, prefixed by project name.",
   "version": "0.0.4",
   "repository": {
      "type": "git",
      "url": "git://github.com/aubin.mahe/vscode-ext-mk.git"
   },
   "engines": {
      "vscode": "^1.74.0"
   },
   "categories": [
      "Other",
      "Programming Languages",
      "SCM Providers"
   ],
   "activationEvents": [
      "onStartupFinished"
   ],
   "main": "./out/extension.js",
   "contributes": {
      "commands": [
         {
            "title": "Makefile",
            "category": "button",
            "command": "Makefile"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/glob": "^8.1.0",
      "@types/mocha": "^10.0.10",
      "@types/node": "22.x",
      "@types/vscode": "^1.96.0",
      "@typescript-eslint/eslint-plugin": "^8.19.1",
      "@typescript-eslint/parser": "^8.19.1",
      "@vscode/test-electron": "^2.4.1",
      "glob": "^11.0.0",
      "mocha": "^11.0.1",
      "typescript": "^5.7.2"
   }
}
